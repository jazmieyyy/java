/* PROGRAM 2
Write java program to display Fibonacci series up to a limit. */

import java.util.*;
class Fibonacci{
    public static void main(String args[]){
        int n,n1,n2,n3,i;
        n1=-1;
        n2=1;
        System.out.print("Enter your limit: ");
        Scanner sc= new Scanner(System.in);
        n=sc.nextInt();
        for(i=0;i<n;i++){
            n3=n1+n2;
            System.out.print(" " +n3);
            n1=n2;
            n2=n3;
        }
    }
}
